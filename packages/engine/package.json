{
  "name": "@logicnomad/engine",
  "version": "0.1.0",
  "description": "Shared engine package for LogicNomad monorepo - Algorithm execution, flowgraph validation, and game logic",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    },
    "./types/flowgraph": {
      "types": "./dist/types/flowgraph.d.ts",
      "import": "./dist/types/flowgraph.js",
      "require": "./dist/types/flowgraph.js"
    },
    "./types/game": {
      "types": "./dist/types/game.d.ts",
      "import": "./dist/types/game.js",
      "require": "./dist/types/game.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.js"
    },
    "./flowgraph": {
      "types": "./dist/flowgraph/index.d.ts",
      "import": "./dist/flowgraph/index.js",
      "require": "./dist/flowgraph/index.js"
    },
    "./executor": {
      "types": "./dist/executor/index.d.ts",
      "import": "./dist/executor/index.js",
      "require": "./dist/executor/index.js"
    },
    "./executor/conditionEvaluator": {
      "types": "./dist/executor/conditionEvaluator.d.ts",
      "import": "./dist/executor/conditionEvaluator.js",
      "require": "./dist/executor/conditionEvaluator.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "logicnomad",
    "shared",
    "engine",
    "algorithm",
    "flowgraph",
    "game-engine"
  ],
  "author": "kyuna0312",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kyuna0312/LogicNomad"
  }
}
