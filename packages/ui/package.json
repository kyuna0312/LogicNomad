{
  "name": "@logicnomad/ui",
  "version": "0.1.0",
  "description": "Shared UI components for LogicNomad monorepo",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./Button": {
      "types": "./dist/components/Button/index.d.ts",
      "import": "./dist/components/Button/index.js",
      "require": "./dist/components/Button/index.js"
    },
    "./Badge": {
      "types": "./dist/components/Badge/index.d.ts",
      "import": "./dist/components/Badge/index.js",
      "require": "./dist/components/Badge/index.js"
    },
    "./Alert": {
      "types": "./dist/components/Alert/index.d.ts",
      "import": "./dist/components/Alert/index.js",
      "require": "./dist/components/Alert/index.js"
    },
    "./Card": {
      "types": "./dist/components/Card/index.d.ts",
      "import": "./dist/components/Card/index.js",
      "require": "./dist/components/Card/index.js"
    },
    "./LoadingSpinner": {
      "types": "./dist/components/LoadingSpinner/index.d.ts",
      "import": "./dist/components/LoadingSpinner/index.js",
      "require": "./dist/components/LoadingSpinner/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:clean": "yarn clean && yarn build",
    "dev": "tsc --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3"
  },
  "keywords": [
    "logicnomad",
    "ui",
    "components",
    "react",
    "shared"
  ],
  "author": "kyuna0312",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kyuna0312/LogicNomad"
  }
}
